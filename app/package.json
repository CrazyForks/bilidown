{
    "name": "bilidown",
    "version": "3.0.1",
    "description": "哔哩哔哩视频解析下载工具",
    "type": "module",
    "main": "js/main.js",
    "scripts": {
        "start": "electron .",
        "dev:ts": "tsc -w",
        "build:ts": "tsc -w",
        "build": "electron-builder"
    },
    "build": {
        "appId": "com.iuroc.bilidown",
        "productName": "Bilidown",
        "files": [
            "**/*"
        ],
        "directories": {
            "buildResources": "assets"
        },
        "win": {
            "target": "nsis"
        },
        "mac": {
            "target": "dmg"
        },
        "linux": {
            "target": "AppImage"
        }
    },
    "devDependencies": {
        "@types/node": "^22.15.21",
        "electron": "^36.2.1",
        "electron-builder": "^26.0.12",
        "typescript": "^5.8.3"
    },
    "homepage": "https://github.com/iuroc/bilidown#readme",
    "bugs": {
        "url": "https://github.com/iuroc/bilidown/issues"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/iuroc/bilidown.git"
    },
    "license": "Apache-2.0",
    "author": "iuroc",
    "keywords": [
        "electron",
        "bilibili",
        "video",
        "download",
        "audio",
        "4k",
        "8k",
        "hdr",
        "dolby-atoms",
        "dolby-vision",
        "mp4",
        "hevc",
        "hi-res",
        "sqlite",
        "typeorm",
        "bootstrap",
        "nodejs",
        "typescript",
        "ffmpeg"
    ]
}